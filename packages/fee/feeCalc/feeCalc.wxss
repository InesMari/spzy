.comlist .item .weight .change{
  width: 30rpx;
  height: 30rpx;
  margin-top: 30rpx;
}
.comlist .item .weight .num{
  line-height: 90rpx;
  margin:0 20rpx;
  font-size: 30rpx;
}
.comlist .item .weight .num input{
  height: 40rpx;
  width: 60rpx;
  text-align: center;
  font-size: 20rpx;
  display: block;
  margin-top: 20rpx;
  border:1rpx solid #d6dbe2;
}

.feeDialog{
  position: absolute;
  top: 50%;
  left: 50%;
  border-radius: 6rpx;
  padding: 10rpx 24rpx;
  background: #fff;
  width: 80%;
  transform: translateX(-50%) translateY(-50%);
}
.feeDialog .header .th,.feeDialog .list .item .td{
  float: left;
  width: 30%;
  line-height: 70rpx;
  font-size: 28rpx;
  text-align: center;
}
.feeDialog .header .th:last-child,.feeDialog .list .item .td:last-child{
  width: 40%;
}
.feeDialog .list .item .td{
  font-size: 26rpx;
}
.feeDialog .list .item{
  border-top: 1rpx dashed #dee2e9;
  position: relative;
}
.feeDialog .list .item .recommend{
  width: 24rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: #ff4400;
  color:#fff;
  text-align: center;
  word-break: break-all;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  font-size: 14rpx;
  line-height: 18rpx;
}
.feeDialog .close{
  position: absolute;
  bottom: -100rpx;
  left: 50%;
  margin-left: -35rpx;
  width: 70rpx;
}